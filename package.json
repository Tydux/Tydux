    "start:angular": "npm run build:tydux-angular -- --watch",
    "test": "run-s build:lib test:lib test:app",
    "test:lib": "jest --config ./jest.lib.config.js",
    "test:app": "jest --config ./jest.app.config.js",
    "test:all": "ng test",
    "cp:license": "cpx LICENSE dist/w11k/tydux && cpx LICENSE dist/w11k/tydux-angular",
    "cp:readme": "cpx README.md dist/w11k/tydux && cpx README.md dist/w11k/tydux-angular",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
