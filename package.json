    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@w11k/ngx-componentdestroyed": "^4.1.4",
    "@w11k/rx-ninja": "^1.8.2",
    "core-js": "^2.5.4",
    "redux": "4.x.x",
