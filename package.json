    "ng": "ng",
    "build": "ng build",
    "build:tydux": "ng build tydux",
    "build:tydux-angular": "ng build tydux-angular",
    "build:lib": "run-s build:tydux build:tydux-angular cp:license cp:readme",
    "start": "npm run start:demo",
    "start:demo": "ng serve",
